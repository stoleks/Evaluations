%------ Molécules odorantes
\definesubmol\geraniol{
  -[::30] (!\lb) !\llh *6( % pied
    !\vide{} !\vide{} (- !\lb OH) % chaine latérale
    =(-)--- %fin du cycle
  )
}
\definesubmol\geraniolSemiDev{
  H_3C - C (- CH_3) =[::90] CH % pied
  !\lh H_2C -[::-60,,2,2] H_2C !\lb C % "cycle"
  (!\lh CH_3) !\llb !\CH -[::30] CH_2 -OH % chaînes latérales
}
\definesubmol\vanilline{
