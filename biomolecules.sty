%------ Drogues
\definesubmol\THC{
  -[::30] *6( % cyle ether
    (-[::120]) -O- *6( % cycle chaine latérale et alcool
      -= (!\lbh !\lbh !\lb) -= (-OH) -
    )
    =- *6(-=(-)---) -- % cycle supérieur
  )
}
\definesubmol\cocaineHaw{
  ? 
    <[::60,0.7] (!\lh N !\lh)
    -[::-60,,,,line width = 3pt]
    >[::-30,0.7] ( % ether-phenyl
      !\lh O !\lb (!\llb O) !\lh *6(=-=-=-)
    )
    -[::130,0.7] ( % ester
      !\ldb (!\llh O)
      !\lb O !\lh 
    )
    -[::80, 0.9] (-[::-30, 0.85]) -[::60, 0.7] 
  ?
}
\definesubmol\bisphenolA{
