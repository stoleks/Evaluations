      *6(- % 3ème cycle
        *5(-- #3) % 4ème cycle
        #4) % 3
      #5) % 2
    #6) % 1
}
\definesubmol\cholesterol{
  HO-[:30] !\sterol {--} {=--} {
    -(-[::-35] (!\lh) !\lb !\lb !\lhb (!\lb) !\lh) -
  } {- (-[::0]) ---} {---} {- (-[::0]) ---}
}
